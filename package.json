{
  "name": "lyft-node",
  "version": "0.0.5",
  "description": "This is a Node.js Wrapper for the Lyft API's public scope endpoints",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "tslint -p .",
    "lint:fix": "npm run lint -- --fix",
    "test": "NODE_ENV=development jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "lint-test": "npm run lint && npm run test:coverage",
    "build": "tsc",
    "prepublishOnly": "npm run lint-test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bschlenk/node-lyft-client.git"
  },
  "keywords": [
    "lyft",
    "rideshare"
  ],
  "author": "Brian Schlenker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bschlenk/node-lyft-client/issues"
  },
  "homepage": "https://github.com/bschlenk/node-lyft-client#readme",
  "dependencies": {
    "dotenv": "6.0.0",
    "enumify": "1.0.4",
    "immutable": "3.8.2",
    "request": "^2.88.0",
    "request-promise": "4.2.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.4",
    "jest": "^23.5.0",
    "nodemon": "^1.18.4",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "directories": {
    "lib": "dist",
    "test": "test"
  }
}
